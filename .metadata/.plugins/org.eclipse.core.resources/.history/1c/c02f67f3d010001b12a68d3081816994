
public class Examples {
public static void main(String[] args) {
	//default values

	int i = 200;

	 

	//cast to a short

	short s = (short)i;

	System.out.println(s);

	 

	//cast to a double

	double d = (double)i;

	System.out.println(d);

	//cast to a byte

	byte b = (byte)i;

	System.out.println(b);	

	
	byte c = 12;

	System.out.println(c);	
	
	
}
}
