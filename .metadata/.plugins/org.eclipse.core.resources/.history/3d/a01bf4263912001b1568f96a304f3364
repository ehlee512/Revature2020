package eg1;

public class Demo1 {
	public static void main(String[] args) {
		/*
		 * datatype		size(bytes)		defaultValue
		 * byte			1byte			0
		 * short		2				0
		 * int			4
		 * long			8
		 * 
		 * float		4				0.0000f
		 * double		8				0.0000d
		 * (any decimal vales in java by default it considers as double)
		 * 
		 * char			2				'\u00000' (UNI-CODE charset) lookup for UNICODE Table
		 *
		 */
		
		
		int x=10;
		System.out.println("x = " + x);
		
		
		float f1=12.3f;
		System.out.println("f1 = "+f1);
		double d1= 22.333d; //d is unnecessary
		
	}

}
