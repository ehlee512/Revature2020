	System.out.print("kddaaad");
	System.out.println("hsasdfasdfelllooooo");

	sayHello();
	
	test a = new test();
	a.saybye();
	
	sayHi();
	
	addition c = new addition();
	c.fromOther();
	c.fromOther();
	abc(4,2);
}


	public static void sayHello() { 
		System.out.println("from public static");
	}
	
	public void saybye() {
		System.out.println("from Non Static");
	}
	
	static void sayHi() {
		System.out.println("from static void ");
	}
	
	public static int abc(int a, int b) {
		
		
		
		
		return a+b;
	}
}
