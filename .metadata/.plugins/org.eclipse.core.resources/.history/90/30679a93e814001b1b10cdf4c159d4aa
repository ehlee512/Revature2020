package p1;


public class array {
	public static void main(String[] args) {
		int a[] = new int[10];
		a[0]= 2;
		
		System.out.println(a[0]);
		System.out.println(a[1]);
		System.out.println(a[3]);

		int min = 0;
		int max = 100;
		
		
		a[2]= (int) (Math.random() * (max - min + 1) + min);
		
		
		System.out.println(a[]);
		
		
		
	}
	
	
}
