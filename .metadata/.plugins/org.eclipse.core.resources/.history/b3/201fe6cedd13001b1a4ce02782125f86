package test;
import numbers.addition;


public class test {
public static void main(String[] args) {
	
	System.out.print("kddaaad");
	System.out.println("hsasdfasdfelllooooo");

	sayHello();
	
	test a = new test();
	a.saybye();
	
	sayHi();
	
	addition c = new addition();
	c.fromOther();
	c.fromOther();
	System.out.println( abc(2,5) );


	test abc = new test();
	abc.sayHi();
	
	
	Integer x;
	x=0; 
	System.out.println(x);
}


	public static void sayHello() { 
		System.out.println("from public static");
	}
	
	public void saybye() {
		System.out.println("from Non Static");
	}
	
	static void sayHi() {
		System.out.println("from static void HItoo ");
	}
	
	public static int abc(int a, int b) {
		
		
		
		
		return a+b;
	}
}
