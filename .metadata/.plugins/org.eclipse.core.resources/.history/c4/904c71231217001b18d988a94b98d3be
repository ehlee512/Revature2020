package p1;

public class checkPrimeNumber {
	public static void main(String[] args) {
		int num = 6;

		int count = 0;
		for (int x = 1; x < (num + 1); x++) {
			if ((num % x) == 0)
				count++;
		}
		if (count == 2)
			System.out.println(num + " is a prime number");
		else
			System.out.println("not a prime nubmer");
	}

}
